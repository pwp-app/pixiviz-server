{
  "name": "pixivc-backend",
  "version": "1.0.0",
  "description": "Backend of pixiv-c",
  "private": true,
  "egg": {
    "declarations": true
  },
  "dependencies": {
    "egg": "^2.15.1",
    "egg-cors": "^2.2.3",
    "egg-redis": "^2.4.0",
    "egg-scripts": "^2.11.0",
    "egg-security": "^2.8.0",
    "egg-validate": "^2.0.2"
  },
  "devDependencies": {
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "egg-bin": "^4.11.0",
    "egg-ci": "^1.11.0",
    "egg-mock": "^4.0.0",
    "eslint": "^7.3.0",
    "eslint-config-egg": "^8.0.1"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "start": "egg-scripts start --daemon --title=egg-server-pixivc-backend",
